package com.example.demo.lock.light;

import org.springframework.util.StopWatch;

/**
 * 轻量级锁
 * @author pc
 *
 */
public class LightDemo {
	
	public static void main(String[] args) {
		B a = new B();
		StopWatch process = new StopWatch();
		process.start();
		for (int i = 0; i < 1000000000L; i++) {
			a.parse();
		}
	}

}
